{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "jsx": "react",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "checkJs": true,
    },
    "include": ["**/*.js"],
    "exclude": [
        "node_modules",
        "**/node_modules/*",
        "middleware/logger/logs",
        "worker_celery",
        "controller/sample code",
        "jsconfig.json",
    ]
}